package com.edu.oop;
/*
 * Object class
 * 
 * */


public class NoteBook {
	//1. field
	public String brand;
	public int price;
	public int serialNumber;
	
	//2. method
	public String getNoteBookInfo() {
		return brand+","+price;
		
		
	//필드에 값을주입하는 기능 추가,
	public void setNoteBook(String brand, int price, int serialNumber) {
		brand = brand;
		price = price;
		serialNumber = serialNumber;
				
		
	}
	
	}
		
}
