	public static void main(String[] args) {
		Product[] pros = {
				new Product("신라면", 2000, 5,"농심"),
				new Product("비타타원", 1500, 5,"농심"),
				new Product("몽쉘", 1000, 2,"농심"),
				new Product("쿠키", 2000, 1,"농심"),
				new Product("과자", 2500, 4,"농심"),
		};
		
		System.out.println("==========1. 상품 정보를 출력합니다. ============");
	}

}
